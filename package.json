    "@number-flow/svelte": "^0.3.9",
    "@o7/icon": "^0.3.70",
    "@oslojs/encoding": "^1.1.0",
    "@prisma/client": "^6.19.1",
    "@tanstack/svelte-query": "^5.90.2",
    "@tanstack/table-core": "^8.21.3",
    "@trpc/client": "^10.45.4",
    "@trpc/server": "^10.45.4",
    "@turf/turf": "^7.3.1",
    "bits-ui": "^2.15.2",
    "clsx": "^2.1.1",
    "cobe": "^0.6.5",
    "date-fns": "^4.1.0",
    "devalue": "^5.6.1",
    "focus-trap": "^7.7.1",
